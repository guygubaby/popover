{
  "name": "popover-starter",
  "version": "0.0.0",
  "description": "popover-starter",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm -r --parallel --filter !playground run dev",
    "build": "pnpm -r --parallel --filter !playground run build",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "pnpm lint -- --fix",
    "play": "pnpm -C playground run dev",
    "build:play": "pnpm -C playground run build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "jest",
    "test:update": "jest -u"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^1.0.14",
    "@iconify-json/carbon": "^1.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vueuse/core": "^8.2.6",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "eslint-plugin-jest": "^26.1.4",
    "esno": "^0.14.1",
    "jest": "^27.5.1",
    "less": "^4.1.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "unocss": "^0.31.6",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-pages": "^0.23.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "dependencies": {
    "@bryce-loskie/utils": "^0.1.27"
  }
